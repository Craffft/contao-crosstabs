{
    "name": "craffft/contao-crosstabs",
    "description": "Crosstabs helper for Contao OpenSource CMS",
    "keywords": ["crosstabs", "crosstable", "cross", "database", "tables"],
    "type": "contao-module",
    "homepage": "http://craffft.de",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "email": "daniel@craffft.de",
            "homepage": "http://craffft.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/contao-crosstabs/issues",
        "source": "https://github.com/Craffft/contao-crosstabs"
    },
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.2,<4-dev",
        "contao-community-alliance/composer-installer": "*"
    },
    "autoload": {
        "classmap": ["system/"]
    },
    "replace": {
        "contao-legacy/crosstabs": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/crosstabs": "system/modules/crosstabs"
            }
        }
    }
}
